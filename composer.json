{
    "name": "johannesschobel/laravel-userhistory",
    "description": "Provide the possibility to provide a history for things a user did.",
    "keywords": [
        "laravel",
        "user history",
        "timeline"
    ],
    "homepage": "https://github.com/johannesschobel/laravel-userhistory",
    "license": "MIT",
    "authors": [
        {
            "name": "Johannes Schobel",
            "email": "johannes.schobel@googlemail.com",
            "homepage": "https://github.com/johannesschobel",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "JohannesSchobel\\UserHistory\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JohannesSchobel\\UserHistory\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
	"minimum-stability": "dev"
}
